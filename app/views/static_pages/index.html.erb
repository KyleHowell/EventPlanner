<%= form_for @event do |f| %>
  Title: <%= f.text_field :title %><br />
  Description: <%= f.text_area :description, size: "60x12" %><br />
  Address: <%= f.text_field :address %><br />
  Latitude: <%= f.text_field :latitude %><br />
  Longitude: <%= f.text_field :longitude %><br />
  <%= f.submit "Submit Event" %>
<% end %>

<div class="container">
  <h1> Event Planner </h1>
  <div id="map-canvas"></div>
</div>

<div class="sidebar">

</div>
